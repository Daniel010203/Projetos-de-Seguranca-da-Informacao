url = str(input("Digite a URL: "))

def verificar_seguranca_url(url):
    # Verifica se a URL começa com "https://"
    if url.startswith("https://"):
        print("URL segura")
    # Verifica se a URL começa com "http://"
    elif url.startswith("http://"):
        print("URL não segura")
    # Verifica se o formato da URL é inválido
    else:
        print("Formato inválido")

verificar_seguranca_url(url)
